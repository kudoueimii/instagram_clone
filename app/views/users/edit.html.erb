<%= form_with(model: @user, local: true ) do |form| %>
 
 <div class="field">
    <%= form.label :image %>
    <%= image_tag(@user.image.url) if @user.image && @user.image.url %>
    <%= form.file_field :image %>
    <%= form.hidden_field :image_cache %>
  </div>

    <div class="actions">
    <%= form.submit %>
  </div>
<% end %>